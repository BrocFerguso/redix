    ["@babel/preset-env", { "loose": true, "modules": false }],
    ["@babel/preset-react", { "useBuiltIns": true }]
  ],
  "plugins": [["@babel/plugin-proposal-class-properties", { "loose": true }]],
  "env": {
    "test": {
      "plugins": ["@babel/transform-modules-commonjs"]
    }
  }
}
